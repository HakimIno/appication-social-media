{
  "name": "poc-app-ui",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules && yarn cache clean && yarn install",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "rm -rf android ios && npx expo prebuild",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:android:local": "eas build --profile development --platform android --local",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "update:dev": "eas update --channel development",
    "update:prod": "eas update --channel production"
  },
